package com.mycompany.tugas_tugas;

/**
 *
 * @author LENOVO
 */
public class PenumpangReguler extends Penumpang {

    public PenumpangReguler(String nama, String noTiket) {
        super(nama, noTiket);
    }

    @Override
    public double hitungHargaTiket() {
        return 50000.0;
    }
    
}
